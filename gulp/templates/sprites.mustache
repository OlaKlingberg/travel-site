/* Do not edit modules/_sprute directly, as it is generated automatically gy Gulp. Instead edit gulp/templates/sprite.js. */

{{#shapes}}
    {{#first}}
        .icon {
            background-image: url('../../assets/images/sprites/{{{sprite}}}');
        }

        .no-svg .icon {
            background-image: url('../../assets/images/sprites/{{#replaceSvgWithPng}}{{{sprite}}}{{/replaceSvgWithPng}}');
        }

    {{/first}}
    .icon--{{base}} {
        width: {{width.outer}}px;
        height: {{height.outer}}px;
        background-position: {{position.relative.xy}};
    }
{{/shapes}}